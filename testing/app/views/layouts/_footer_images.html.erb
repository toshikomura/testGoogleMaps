<div id="footer-images">
  <ul>
    <li><%= image_tag(@logo_prefeitura,
            :alt => "Logo prefeitura", :size => "225x92",
            :hidden => @contraste.present?, :class => "img-padrao") %>
    </li>
    <li><%= image_tag(@logo_contraste_prefeitura,
            :alt => "Logo alto contraste prefeitura", :size => "225x92",
            :hidden => @contraste.nil?, :class => "img-alto-contraste") %>
    </li>
    <li><%= link_to image_tag("marca_ministerio-das-comunicacoes.png",
            :alt => "Logo ministério das comunicações",
            :size => "405x73"), "http://www.mc.gov.br",
            {:hidden => @contraste.present?, :class => "img-padrao"} %>
    </li>
    <li><%= link_to image_tag("logo-contraste-ministerio.png",
            :alt => "Logo alto contraste ministério das comunicações",
            :size => "404x92"), "http://www.mc.gov.br",
            {:hidden => @contraste.nil?, :class => "img-alto-contraste"} %>
    </li>
  </ul>
</div>
